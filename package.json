{
  "name": "hermes",
  "version": "1.0.0",
  "description": "",
  "main": "server/server.js",
  "scripts": {
    "build:dev": "webpack --env=dev --progress --profile --colors",
    "build:dist": "webpack --env=prod --progress --profile --colors",
    "start": "nodemon --watch server --exec babel-node -- glimpse.js",

    "integration": "NODE_ENV=test mocha --timeout 50000 --compilers js:babel-core/register server/tests/**/*.spec.js",
    "coverage": "nyc report --reporter=text-lcov |  coveralls",
    "db:migrate:test": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate",
    "db:seed:test": "NODE_ENV=test sequelize db:seed:all",
    "test:integration": "npm run db:migrate:test && npm run db:seed:test && npm run integration",
    "test": "npm run integration && npm run coverage",

    "db:migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "postinstall": "npm run build && npm run db:migrate && npm run db:seed"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "es6",
      "js"
    ],
    "collectCoverage": true
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
